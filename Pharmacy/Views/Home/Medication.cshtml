@model  List<Pharmacy.Models.PharmacyMed>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>АПТЕКИ</title>
</head>
<body>
    <div>
        <h3>Аптеки: </h3>
        <table>
            <tr>
                <td><p>Название </p></td>
                <td><p>Вес </p></td>
                <td><p>Количество </p></td>
                <td><p>Описание </p></td>
                <td><p>Цена </p></td>
                <td></td>
            </tr>
            @foreach (var b in Model)
            {
                <tr>
                    <td><p>@b.Medication.Name</p></td>
                    <td><p>@b.Medication.Weight</p></td>
                    <td><p>@b.Medication.Quantity</p></td>
                    <td><p>@b.Medication.Description</p></td>
                    <td><p>@b.Medication.Price</p></td>
                </tr>
            }
        </table>
    </div>
</body>
</html>
